    "fast-html-parser": "^1.0.1",
    "istanbul": "^0.4.4",
    "mkdirp": "^0.5.1",
    "mocha": "^2.5.3",
    "nopt": "^3.0.6",
    "postcss-cli": "^2.5.2",
    "uglifyjs": "^2.4.10"
